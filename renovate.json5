{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchManagers": ["gradle-wrapper", "maven-wrapper"],
      "matchUpdateTypes": ["major", "minor"],
      "matchFileNames": [
        "/(?:gradle|\.mvn)/wrapper/"
      ],
      "enabled": false,
    },
  ]
}
